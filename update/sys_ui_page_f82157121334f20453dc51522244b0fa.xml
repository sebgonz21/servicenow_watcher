	<body id="body" ng-app='watcher'>
		
		<div ng-controller = 'watcherCtrl'>
			
			<div class=" col-lg-12 col-md-12col-sm-12 col-xs-12 header-container">
				<div class=" col-lg-8 col-md-8 col-sm-12 col-xs-12 title-container">
					<h4>Locate Application Servers</h4>
				</div>
				<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 date-title-container">
						<h4>Date</h4>
					</div>
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 date-container">
						<input ng-model='date' type="date" required="required" class="form-control"/>
					</div>					
					
				</div>
			</div>
				
			<div class=" col-lg-12 col-md-12 col-sm-12">
				<h2>Production</h2>
				
				
				<!-- EXTERNAL -->
				<div id="external_group" class="group-box col-lg-12 col-md-12 col-sm-12 col-xs-12">
					<p class='group-label'>External</p>
					<div class="odd-five my-container col-lg-2 col-md-2 col-sm-12" ng-repeat="mon in prod_monitors | filter: 'external'">
						
						<server-monitor server="{{mon.server}}" monitor_id="{{mon.sys_id}}" status="{{mon.status}}" ></server-monitor>

					</div>
					
				</div>
				<!-- EXTERNAL -->
				
				<!-- PRIMARY -->
				<div id="primary_group" class="group-box col-lg-3 col-md-3 col-sm-12 col-xs-12">
					<p class='group-label'>Primary</p>
					<div class="my-container col-lg-12 col-md-12 col-sm-12" ng-repeat="mon in prod_monitors | filter: 'primary'">
						
						<server-monitor server="{{mon.server}}" monitor_id="{{mon.sys_id}}" status="{{mon.status}}" ></server-monitor>
						
					</div>
				</div>
				
				<!-- PRIMARY -->
				
				<!-- INTERNAL -->
				<div id="internal_group" class="group-box col-lg-9 col-md-9 col-sm-12 col-xs-12">
					<p class='group-label'>Internal</p>
					<div class="my-container col-lg-4 col-md-4 col-sm-12 col-xs-12" ng-repeat="mon in prod_monitors | filter: 'internal'">
						<server-monitor server="{{mon.server}}" monitor_id="{{mon.sys_id}}" status="{{mon.status}}" ></server-monitor>

					</div>
				</div>
				<!-- INTERNAL -->
				
				<!-- AVAILABLE -->
				<div id="available_group" class="group-box col-lg-3 col-md-3 col-sm-12 col-xs-12">
					<p class='group-label'>Available</p>
					<div class="my-container col-lg-12 col-md-12 col-sm-12" ng-repeat="mon in prod_monitors | filter: 'available'">
						
						<server-monitor server="{{mon.server}}" monitor_id="{{mon.sys_id}}" status="{{mon.status}}" ></server-monitor>

					</div>
					
				</div>
				<!-- AVAILABLE -->
				
			</div>
			
			
		</div>
	</body>
